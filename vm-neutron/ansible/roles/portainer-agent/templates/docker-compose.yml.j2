services:
  portainer_agent:
    image: portainer/agent
    container_name: portainer_agent
    restart: always
    ports:
      - "9001:9001"
    volumes:
      - "/var/run/docker.sock:/var/run/docker.sock"
      - "/var/lib/docker/volumes:/var/lib/docker/volumes"
    networks:
      - vm-docker-30-net

networks:
  vm-docker-30-net:
    driver: bridge
